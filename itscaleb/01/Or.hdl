/**
 * Or gate: out = 1 if {a==1 or b==1}, 0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a = a, b = a, out = w0);
    Nand(a = b, b = b, out = w1);
    Nand(a = w0, b = w1, out = out);
}
