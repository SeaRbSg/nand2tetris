/**
 * 16-bit incrementer: out = in + 1 (arithmetic addition)
 */

CHIP Inc16 {
    IN in[16];
    OUT out[16];

    PARTS:
    Not16(in=in, out=notin);
    Add16(a=notin, b=true, out=w1);
    Not16(in=w1, out=out);
}