/**
 * Demultiplexor.
 * {a,b} = {in,0} if sel==0
 *         {0,in} if sel==1
 */


CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a=sel, b=sel, out=notsel);
    Nand(a=notsel, b=in, out=w1);
    Nand(a=w1, b=w1, out=a);
    Nand(a=sel, b=in, out=w2);
    Nand(a=w2, b=w2, out=b);

}
